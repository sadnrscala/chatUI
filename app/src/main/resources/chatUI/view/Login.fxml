<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<HBox maxHeight="150.0" maxWidth="350.0" minHeight="150.0" minWidth="350.0" prefHeight="150.0" prefWidth="350.0" styleClass="root" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="chatUI.view.LoginControllerImpl">
   <children>
      <VBox prefHeight="479.0" prefWidth="1027.0" spacing="10.0" HBox.hgrow="ALWAYS">
         <children>
            <Label alignment="CENTER" contentDisplay="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="Pick your nickname" VBox.vgrow="ALWAYS" />
            <TextField fx:id="loginInput" alignment="CENTER" />
            <Button onAction="#onLoginAction" fx:id="loginButtonLogin" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Accept" />
            <Label fx:id="loginLabelError" />
         </children>
      </VBox>
   </children>
   <stylesheets>
      <URL value="@ChatWindow.css" />
   </stylesheets>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</HBox>
